{
  "name": "@jsonstack/data",
  "version": "2.0.0",
  "description": "jsonstack data is a TypeScript module with simple and efficient tools for data mining and data analysis in TypeScript. When jsonstack data used with ML.js, pandas-js, and numjs, you're left with the equivalent R/Python tool set in TypeScript.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/web/index.js",
  "web": "dist/web/web.js",
  "modules.root": "dist/esm",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/repetere/jsonstack-data.git"
  },
  "author": {
    "name": "Yaw Joseph Etse",
    "email": "yaw.etse@gmail.com"
  },
  "keywords": [
    "Machine Learning",
    "data science",
    "scaling",
    "feature scaling",
    "feature engineering",
    "scalars",
    "scikit-learn",
    "sk-learn",
    "ml",
    "pre-processing",
    "data preparation",
    "pandas",
    "numpy"
  ],
  "bugs": {
    "url": "https://github.com/repetere/jsonstack-data/issues"
  },
  "homepage": "https://github.com/repetere/jsonstack-data#readme",
  "scripts": {
    "dev": "node esbuild.config.js -w",
    "esbuild": "node esbuild.config.js",
    "benchmark": "node -r esm  benchmark/main_bmrk.js",
    "test": "jest --coverage",
    "doc": "tsc && typedoc && sitedown manual/ -b docs/manual",
    "compile": "node esbuild.config.js",
    "build": "tsc && typedoc && sitedown manual/ -b docs/manual",
    "coverage": "coveralls"
  },
  "dependencies": {
    "csvtojson": "^2.0.10",
    "js-grid-search-lite": "^1.3.3",
    "lodash.range": "^3.2.0",
    "lodash.rangeright": "^4.2.0",
    "ml-confusion-matrix": "^0.4.0",
    "ml-matrix": "^6.4.1",
    "ml-stat": "^1.3.3",
    "node-fpgrowth": "^1.2.1",
    "probability-distributions": "^0.9.1",
    "random-js": "^2.1.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@geut/esbuild-plugin-brode": "^2.0.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^28.1.4",
    "@types/lodash.range": "^3.2.7",
    "@types/lodash.rangeright": "^4.2.7",
    "@types/natural": "^5.1.1",
    "@types/probability-distributions": "0.1.0",
    "@types/stemmer": "^2.0.0",
    "@types/valid-url": "1.0.3",
    "esbuild": "^0.14.47",
    "esbuild-node-builtins": "^0.1.0",
    "husky": "^8.0.1",
    "install": "0.13.0",
    "jest": "^28.1.2",
    "ml-cart": "^2.0.2",
    "ml-knn": "3.0.0",
    "ml-random-forest": "^2.0.2",
    "ml-regression-multivariate-linear": "2.0.3",
    "puppeteer": "^15.3.2",
    "stemmer": "1.0.5",
    "ts-jest": "^28.0.5",
    "tslib": "^2.1.0",
    "typedoc": "^0.23.7",
    "typescript": "^4.7.4",
    "underscore": "^1.12.1"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "extensionsToTreatAsEsm": [
          ".ts",
          ".js"
        ],
        "useESM": true,
        "diagnostics": false,
        "tsconfig": "tsconfig.jest.json"
      }
    },
    "transformIgnorePatterns": [
      "node_modules/?!(scikitjs)"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "Release: ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "major"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "breaking",
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  }
}
